{
    "configHelpUrl": "http://usejsdoc.org/about-configuring-jsdoc.html",
    "script": "jsdoc --configure ./.jsdoc.json --verbose",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "source": {
        "include": ["docs/readme.md", "src"],
        "exclude": [],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "markdown": {
        "excludeTags": [
            "author"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/better-docs",
        "readme": "./docs/readme.md"
    }
}